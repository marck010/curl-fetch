{
  "author": {
    "name": "Marcos de Matos",
    "email": "marck010@hotmail.com"
  },
  "name": "fetch-curl",
  "version": "0.5.0",
  "description": "node-libcurl abstraction to api fetch",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "ISC",
  "repository": {
    "url": "https://github.com/marck010/curl-fetch.git"
  },
  "keywords": [
    "node-curl",
    "curl",
    "libcurl",
    "node-libcurl",
    "fetch-curl",
    "fetch-http2"
  ],
  "dependencies": {
    "http-status": "^1.3.2",
    "node-libcurl": "^2.0.1",
    "url": "^0.11.0"
  },
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai": "^4.2.1",
    "@types/express": "^4.17.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.5",
    "@types/sinon": "^7.0.13",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "express": "^4.17.1",
    "gh-pages": "^2.1.1",
    "mocha": "^10.2.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "sinon": "^7.4.2",
    "source-map-support": "^0.5.13",
    "spdy": "^4.0.1",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.3"
  },
  "scripts": {
    "clear": "rimraf ./dist",
    "prebuild": "npm run clear",
    "build": "tsc --build ./tsconfig.json",
    "build:watch": "tsc --watch",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "SERVER_PORT=8231 ts-mocha --exit tests/**/test.ts",
    "unit-test": "ts-mocha --exit tests/unit-tests/test.ts",
    "api-test": "SERVER_PORT=8231 ts-mocha --exit tests/api-tests/test.ts",
    "coverage": "nyc npm run test",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm test && npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "docs": "typedoc --out docs src",
    "gh-pages": "rimraf docs && npm run docs && gh-pages -d docs"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 90,
    "functions": 90,
    "branches": 90,
    "lines": 90
  }
}
